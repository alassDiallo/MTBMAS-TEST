             rdfs:label "Owner" .


###  http://mtbmas#Person
mtbmas:Person rdf:type owl:Class ;
              rdfs:label "Person" .


###  http://mtbmas#Room
mtbmas:Room rdf:type owl:Class ;
            rdfs:subClassOf mtbmas:Space .


###  http://mtbmas#Service
mtbmas:Service rdf:type owl:Class ;
               rdfs:subClassOf saref:Service ;
               rdfs:label "Service" .


###  http://mtbmas#ServiceAgentManager
mtbmas:ServiceAgentManager rdf:type owl:Class ;
                           owl:disjointUnionOf ( mtbmas:ServiceAirQualityAgentManager
                                                 mtbmas:ServiceCoolingAgentManager
                                                 mtbmas:ServiceHeatingAgentManager
                                                 mtbmas:ServiceLightingAgentManager
                                                 mtbmas:ServiceVentilationAgentManager
                                               ) ;
                           rdfs:label "Service Agent Manager" .


###  http://mtbmas#ServiceAirQualityAgentManager
mtbmas:ServiceAirQualityAgentManager rdf:type owl:Class ;
                                     rdfs:subClassOf mtbmas:ServiceAgentManager .


###  http://mtbmas#ServiceCoolingAgentManager
mtbmas:ServiceCoolingAgentManager rdf:type owl:Class ;
                                  rdfs:subClassOf mtbmas:ServiceAgentManager .


###  http://mtbmas#ServiceHeatingAgentManager
mtbmas:ServiceHeatingAgentManager rdf:type owl:Class ;
                                  rdfs:subClassOf mtbmas:ServiceAgentManager .


###  http://mtbmas#ServiceLightingAgentManager
mtbmas:ServiceLightingAgentManager rdf:type owl:Class ;
                                   rdfs:subClassOf mtbmas:ServiceAgentManager .


###  http://mtbmas#ServiceVentilationAgentManager
mtbmas:ServiceVentilationAgentManager rdf:type owl:Class ;
                                      rdfs:subClassOf mtbmas:ServiceAgentManager .


###  http://mtbmas#Simulation
mtbmas:Simulation rdf:type owl:Class ;
                  rdfs:label "Simulation" .


###  http://mtbmas#SimulationResult
mtbmas:SimulationResult rdf:type owl:Class ;
                        rdfs:label "Simulation Result" .


###  http://mtbmas#Space
mtbmas:Space rdf:type owl:Class ;
             rdfs:subClassOf s4bldg:BuildingSpace ;
             rdfs:label "Space" .


###  http://mtbmas#SpaceSimulationModel
mtbmas:SpaceSimulationModel rdf:type owl:Class ;
                            rdfs:label "Space Simulation Model" .


###  http://mtbmas#Tenant
mtbmas:Tenant rdf:type owl:Class ;
              rdfs:label "Tenant" .


###  http://mtbmas#Visitor
mtbmas:Visitor rdf:type owl:Class ;
               rdfs:subClassOf mtbmas:Person ;
               rdfs:label "Visitor" .


###  http://mtbmas#Zone
mtbmas:Zone rdf:type owl:Class ;
            rdfs:subClassOf mtbmas:Space .


###  https://saref.etsi.org/core/v3.2.1/Device
saref:Device rdf:type owl:Class ;
             rdfs:label "Device" .


###  https://saref.etsi.org/core/v3.2.1/Service
saref:Service rdf:type owl:Class ;
              rdfs:label "Service" .


###  https://saref.etsi.org/saref4bldg/Building
