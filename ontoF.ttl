#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#backwardCompatibleWith
owl:backwardCompatibleWith rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#incompatibleWith
owl:incompatibleWith rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#priorVersion
owl:priorVersion rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#versionInfo
owl:versionInfo rdf:type owl:AnnotationProperty .


###  https://w3id.org/mod#createdWith
mod:createdWith rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://mtbmas#canAcess
mtbmas:canAcess rdf:type owl:ObjectProperty ;
                rdfs:domain mtbmas:Person ;
                rdfs:label "can acess" .


###  http://mtbmas#canBe
mtbmas:canBe rdf:type owl:ObjectProperty ;
             rdfs:domain mtbmas:Owner ;
             rdfs:range mtbmas:Tenant ;
             rdfs:label "can be" .


###  http://mtbmas#canUse
mtbmas:canUse rdf:type owl:ObjectProperty ;
              rdfs:domain mtbmas:Person ;
              rdfs:range mtbmas:Service ;
              rdfs:label "can use" .


###  http://mtbmas#contains
mtbmas:contains rdf:type owl:ObjectProperty ;
                rdfs:range mtbmas:Device ;
                rdfs:label "contains" .


###  http://mtbmas#containsAgent
mtbmas:containsAgent rdf:type owl:ObjectProperty ;
                     owl:inverseOf mtbmas:locatedIn ;
                     rdfs:range mtbmas:ServiceAgentManager ;
                     rdfs:label "contains agent" .


###  http://mtbmas#controlsDevice
mtbmas:controlsDevice rdf:type owl:ObjectProperty ;
                      owl:inverseOf mtbmas:isControledBy ;
                      rdfs:domain mtbmas:ServiceAgentManager ;
                      rdfs:range mtbmas:Device ;
                      rdfs:label "controls device" .


###  http://mtbmas#hasOccupiant
mtbmas:hasOccupiant rdf:type owl:ObjectProperty ;
                    rdfs:domain mtbmas:Tenant ;
                    rdfs:range mtbmas:Occupant ;
                    rdfs:label "has occupiant" .


###  http://mtbmas#hasPerson
mtbmas:hasPerson rdf:type owl:ObjectProperty ;
                 rdfs:domain mtbmas:Building ;
                 rdfs:range mtbmas:Person ;
                 rdfs:label "has person" .


###  http://mtbmas#hasResult
mtbmas:hasResult rdf:type owl:ObjectProperty ;
                 owl:inverseOf mtbmas:isResultOf ;
                 rdfs:domain mtbmas:Simulation ;
                 rdfs:range mtbmas:SimulationResult ;
                 rdfs:label "has result" .


###  http://mtbmas#hasSimulationModel
mtbmas:hasSimulationModel rdf:type owl:ObjectProperty ;
                          owl:inverseOf mtbmas:isSimModelOf ;
                          rdfs:range mtbmas:SpaceSimulationModel ;
                          rdfs:label "has simulation model" .


###  http://mtbmas#hasSpace
mtbmas:hasSpace rdf:type owl:ObjectProperty ;
                owl:inverseOf mtbmas:isSspaceOf ;
                rdf:type owl:TransitiveProperty ;
                rdfs:domain mtbmas:Building ,
                            mtbmas:Space ;
                rdfs:range mtbmas:Space ;
                rdfs:label "has space" .


###  http://mtbmas#hasTenant
mtbmas:hasTenant rdf:type owl:ObjectProperty ;
                 rdfs:domain mtbmas:Building ;
                 rdfs:range mtbmas:Tenant .


###  http://mtbmas#hasVisitor
mtbmas:hasVisitor rdf:type owl:ObjectProperty ;
                  rdfs:domain mtbmas:Tenant ;
                  rdfs:range mtbmas:Visitor ;
                  rdfs:label "has visitor" .


###  http://mtbmas#interactWith
mtbmas:interactWith rdf:type owl:ObjectProperty ;
                    rdfs:domain mtbmas:Tenant ;
                    rdfs:range mtbmas:ServiceAgentManager ;
                    rdfs:label "interact with" .


###  http://mtbmas#isControledBy
mtbmas:isControledBy rdf:type owl:ObjectProperty ;
                     rdfs:domain mtbmas:Device ;
                     rdfs:range mtbmas:ServiceAgentManager ;
                     rdfs:label "is Controled By" .


###  http://mtbmas#isOfferedBy
mtbmas:isOfferedBy rdf:type owl:ObjectProperty ;
                   owl:inverseOf mtbmas:offers ;
                   rdfs:domain saref:Service ;
                   rdfs:range mtbmas:Device ,
                              mtbmas:ServiceAgentManager .


###  http://mtbmas#isOwnedBy
mtbmas:isOwnedBy rdf:type owl:ObjectProperty ;
                 owl:inverseOf mtbmas:owns ;
                 rdfs:domain mtbmas:Building ,
                             mtbmas:Space ;
                 rdfs:range mtbmas:Owner .


###  http://mtbmas#isResultOf
mtbmas:isResultOf rdf:type owl:ObjectProperty ;
                  rdfs:domain mtbmas:SimulationResult ;
                  rdfs:range mtbmas:Simulation .


###  http://mtbmas#isSimModelOf
mtbmas:isSimModelOf rdf:type owl:ObjectProperty ;
                    rdfs:domain mtbmas:SpaceSimulationModel ;
                    rdfs:range mtbmas:Space .


###  http://mtbmas#isSspaceOf
mtbmas:isSspaceOf rdf:type owl:ObjectProperty ;
                  rdfs:domain mtbmas:Space ;
                  rdfs:range mtbmas:Building ,
                             mtbmas:Space .


###  http://mtbmas#isTenantOf
mtbmas:isTenantOf rdf:type owl:ObjectProperty ;
                  rdfs:domain mtbmas:Tenant ;
                  rdfs:range mtbmas:Building ;
                  rdfs:label "is tenant of" .


###  http://mtbmas#isUsedBy
mtbmas:isUsedBy rdf:type owl:ObjectProperty ;
                owl:inverseOf mtbmas:useResult ;
                rdfs:domain mtbmas:SimulationResult ;
                rdfs:range mtbmas:ServiceAgentManager .


###  http://mtbmas#locatedIn
mtbmas:locatedIn rdf:type owl:ObjectProperty ;
                 owl:inverseOf s4bldg:contains ;
                 rdfs:domain mtbmas:ServiceAgentManager ;
                 rdfs:range mtbmas:Space .


###  http://mtbmas#occupies
mtbmas:occupies rdf:type owl:ObjectProperty ;
                rdfs:domain mtbmas:Tenant ;
                rdfs:label "occupies" .


###  http://mtbmas#offers
mtbmas:offers rdf:type owl:ObjectProperty ;
              rdfs:domain mtbmas:Device ;
              rdfs:range mtbmas:Service ;
              rdfs:label "offers" .


###  http://mtbmas#offersSpecificService
mtbmas:offersSpecificService rdf:type owl:ObjectProperty ;
                             rdfs:domain mtbmas:ServiceAgentManager ;
                             rdfs:range mtbmas:Service ;
                             rdfs:label "offers specific service" .


###  http://mtbmas#owns
mtbmas:owns rdf:type owl:ObjectProperty ;
            rdfs:domain mtbmas:Owner ;
            rdfs:range mtbmas:Building ;
            rdfs:label "owns" .


###  http://mtbmas#producedBy
mtbmas:producedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf mtbmas:producesSim ;
                  rdfs:domain mtbmas:Simulation ;
                  rdfs:range mtbmas:SpaceSimulationModel .


###  http://mtbmas#producesSim
mtbmas:producesSim rdf:type owl:ObjectProperty ;
                   rdfs:domain mtbmas:SpaceSimulationModel ;
                   rdfs:range mtbmas:Simulation ;
                   rdfs:label "produces sim" .


###  http://mtbmas#providesInputDataSimulation
mtbmas:providesInputDataSimulation rdf:type owl:ObjectProperty ;
                                   rdfs:domain mtbmas:ServiceAgentManager ;
                                   rdfs:range mtbmas:Simulation ;
                                   rdfs:label "provides Input Data Simulation" .


###  http://mtbmas#useResult
mtbmas:useResult rdf:type owl:ObjectProperty ;
                 rdfs:domain mtbmas:ServiceAgentManager ;
                 rdfs:range mtbmas:SimulationResult ;
                 rdfs:label "use result" .


###  https://saref.etsi.org/saref4bldg/contains
s4bldg:contains rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://mtbmas#input_val
mtbmas:input_val rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain mtbmas:Simulation ;
                 rdfs:range xsd:double .


###  http://mtbmas#predictValue
mtbmas:predictValue rdf:type owl:DatatypeProperty ;
                    rdfs:domain mtbmas:SimulationResult ;
                    rdfs:range xsd:double .


###  http://mtbmas#weather
mtbmas:weather rdf:type owl:DatatypeProperty ;
               rdfs:domain mtbmas:Simulation ;
               rdfs:range xsd:double .


#################################################################
#    Classes
#################################################################

###  http://mtbmas#Building
mtbmas:Building rdf:type owl:Class ;
                rdfs:subClassOf s4bldg:Building ;
                rdfs:label "Building" .


###  http://mtbmas#Device
mtbmas:Device rdf:type owl:Class ;
              rdfs:subClassOf saref:Device ;
              rdfs:label "Device" .


###  http://mtbmas#Floor
mtbmas:Floor rdf:type owl:Class ;
             rdfs:subClassOf mtbmas:Space .


###  http://mtbmas#Occupant
mtbmas:Occupant rdf:type owl:Class ;
                rdfs:subClassOf mtbmas:Person ;
                rdfs:label "Occupant" .


###  http://mtbmas#Owner
